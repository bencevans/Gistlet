{
  "name": "Gistlet",
  "description": "Add a link to a dabblet.com view of a GitHub Gist",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {},
  "permissions": [
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gist.github.com/*"
      ],
      "js": [
        "gist.js"
      ]
    }
  ]
}